    "@repo/utils": "^0.0.0",
    "aws-jwt-verify": "^5.0.0",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.0",
    "jsonwebtoken": "^9.0.2",
    "otplib": "^12.0.1",
