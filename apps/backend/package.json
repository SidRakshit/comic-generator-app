  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node -r dotenv/config src/index.ts",
    "start": "node dist/index.js",
    "build": "npm run build:deps && prisma generate && tsc",
    "build:deps": "cd ../../packages/utils && npm run build && cd ../../packages/common-types && npm run build",
    "lint": "eslint src --ext .ts --config ../../packages/eslint-config/node.js --ignore-pattern 'src/generated/**/*'",
    "migrate:dev": "prisma migrate dev",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:resolve": "prisma migrate resolve",
    "admin:token": "ts-node src/scripts/generate-service-token.ts"
  },
